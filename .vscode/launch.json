{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "octave",
            "request": "launch",
            "name": "Octave Debug Adapter(preview)",
            "exec": "octave-cli",
            "program": "${command:AskForProgramName}"
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "ex4",
            "program": "ex4",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        }
    ]
}